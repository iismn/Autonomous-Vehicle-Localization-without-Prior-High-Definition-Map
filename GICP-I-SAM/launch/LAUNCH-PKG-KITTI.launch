<launch>
    <arg name="project" default="agv_global_module"/>

    <!-- Parameters -->
    <rosparam file="$(find agv_global_module)/config/Params_KITTI.yaml"             command="load" />
    <rosparam file="$(find agv_global_module)/config/Params_Sensor_KITTI.yaml" 	    command="load"/>
    <!--- LOAM -->
    <include file="$(find agv_global_module)/launch/include/module_gicp_single.launch" />

    <!--- Robot State TF -->
    <include file="$(find agv_global_module)/launch/include/module_robot_state_publisher.launch" />

    <!--- Run Navsat -->
    <include file="$(find agv_global_module)/launch/include/module_navsat.launch" />

    <!--- Run Rviz-->
    <include file="$(find agv_global_module)/launch/include/module_rviz.launch" />



  	<node name="vehicle_tf_node" pkg="vehicle_tf" type="vehicle_tf_node" output="screen">
        	<rosparam command="load" file="$(find vehicle_tf)/config/tf_config.yaml" />
  				<param name="bag" value="true"/>
  	</node>


</launch>
